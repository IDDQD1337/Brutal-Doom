	+FORCEPAIN
	//+RIPPER
	RenderStyle "Add"
	Alpha 0.6
    Damage (random(20,25))//(random(90,95))
    Speed 30//60
	SeeSound "dsempty"
	DeathSound "dsempty"
	decal "AxeScrape"
