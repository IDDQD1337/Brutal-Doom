ACTOR BasicWeaponPickup : Health
{
	scale 0.8
	radius 16 //Anything above this value will cause the actor to get stuck on platforms. DO NOT CHANGE.
	+FORCEXYBILLBOARD
	+MOVEWITHSECTOR
	-COUNTITEM
	+INVENTORY.ALWAYSPICKUP
	Inventory.Amount 0
	Inventory.MaxAmount 99999
	Inventory.PickupMessage ""
	Inventory.PickupSound "dsempty"
	+Quiet
}

Actor BasicWeaponPickup2: BasicWeaponPickup
{
	-INVENTORY.ALWAYSPICKUP
	Inventory.MaxAmount 0
}


// Backpack ---------------------------------------------------------------

ACTOR AmmoSuply : BackpackItem  Replaces Backpack
{
	//$Category Ammunition
	//$Sprite BPAKA0
	//$Title "Backpack"
	//$Arg0 Spawn extra guns? 
	//$Arg0Tooltip Select whether to force a chance for extra weapon spawns for this pickup.
	//$Arg0Type 11
	//$Arg0Enum { 0 = "Configurable by user (DEFAULT)"; 1 = "Force random extra weapon"; 2 = "Force SMG"; 3 = "Force ASG"; 4 = "Force Sniper"; 5 = "Force Grenade Launcher"; 6 = "Force Railgun"; 7 = "Force Flamethrower"; 8 = "Force Nuke Launcher"; 9 = "Force BFG10k"; }
	Game Doom
	SpawnID 144
	Height 26
	Inventory.PickupMessage "$GOTBACKPACK"
	Inventory.PickupSound "BACKPACK"
	+FORCEXYBILLBOARD
	States
	{

	Spawn:
		BPAK A 0
		TNT1 A 0 A_JumpIf(args[0] == 1, "BDV22Style")
		BPAK A 0 ACS_NamedExecuteAlways("AmmoSupplyChecker", 0, 0, 0, 0)
		BPAK A 2
	Vanilla:
	Spawn1:
	Stay:
		BPAK A 0
		BPAK A -1
		Stop

	BDV22Style:
		TNT1 A 0
		TNT1 A 1 A_SpawnItemEx("NewGunsSpawner", random(-10, 10), random(-10, 10), 0,0,0,0,0,SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG | SXF_TRANSFERSPECIAL,0)
		Goto Stay

	}
}


