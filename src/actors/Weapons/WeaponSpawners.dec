	Game Doom
	+THRUACTORS
	+CANNOTPUSH
	-COUNTITEM
	-INVENTORY.ALWAYSPICKUP
	+INVENTORY.NEVERRESPAWN
	+FORCEXYBILLBOARD
	Inventory.Amount 5
	Inventory.MaxAmount 150
	Tag "Demonic Essence"
	Inventory.PickupMessage "$GOTESSENCE"
	Scale 0.7
	States
	{
	Spawn:
		ZHHP A 0
