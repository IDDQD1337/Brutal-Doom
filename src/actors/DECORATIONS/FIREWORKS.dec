ACTOR BarrelShrapnel1
{
	Radius 2
	Height 2
	Speed 20
	Mass 1
	+NOBLOCKMAP
	+MISSILE
	+NOTELEPORT
	+MOVEWITHSECTOR
	+CLIENTSIDEONLY
	+THRUACTORS
	+FLOORCLIP
	+DOOMBOUNCE
	-NOGRAVITY
	+FORCEXYBILLBOARD
	BounceFactor 0.5
	Gravity 0.9
	Mass 1
	States
	{
	Spawn:
		TNT1 A 0 NODELAY A_SetScale(frandom(1.0,1.7))
		TNT1 A 0 A_JumpIf(waterlevel > 1, "Underwater")
		BRPT ABCDEFGH 1
		TNT1 A 0 A_ChangeFlag("NOGRAVITY", 0)
		Goto Spawn2

	Spawn2:
		TNT1 A 0 A_JumpIf(waterlevel > 1, "Underwater")
		BRPT ABCDEFGH 2
		Loop

	Death:
		TNT1 A 0
		BRPT I 300
		BRPT IIIIIIIII 2 A_FadeOut(0.1)
		Stop

	Underwater:
	Splash:
		Goto Death
	}
}

