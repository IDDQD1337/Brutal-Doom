Actor TallTorch_Red : RedTorch replaces RedTorch
{
	height 85
	radius 2
	+FORCEYBILLBOARD
	scale 1.0
	states
	{
	Vanilla:
		TNT1 A 0 A_NoBlocking
		TNT1 A 0 A_SpawnItem("RedTorchVanilla")
		Stop

	Spawn:
		TTRB A 0
		TNT1 A 0 A_Jump(128,2)
		TNT1 A 0 A_SetScale(-scalex,scaley)
		TNT1 A 0
		TNT1 A 0 A_Playsound("BDFireplace2", 1, 1, 1)
		TTRB A 2 ACS_NamedExecuteAlways("BDCheckDecorations", 0, 0, 0, 0)//Check if advanced decorations are disabled.
		TTRB A 2
		TNT1 A 0 A_SPawnItemEX("TallTOrch_RedFlames",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION|SXF_CLIENTSIDE)
		TNT1 A 0 A_SPawnItemEX("TallTOrch_RedCore",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION|SXF_CLIENTSIDE)
		TTRB A 2 A_SPawnItemEX("TorchBall",0,0,0,0,0,0,0,SXF_NOCHECKPOSITION|SXF_CLIENTSIDE)
		TTRB A -1
		stop
	}
}

