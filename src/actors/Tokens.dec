{
	Inventory.MaxAmount 1
	+inventory.Undroppable
	-inventory.Invbar
	+NONETID
	+INVISIBLE
	+NOCLIP
	+NOTARGET
	+NOBLOCKMAP
	+NOINTERACTION
	Radius 0
	Height 0
	Alpha 0.0
	Mass 0
	RenderStyle "None"
}

