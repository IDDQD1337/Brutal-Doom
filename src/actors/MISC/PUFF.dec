ACTOR HitPuff Replaces BulletPuff
{
	renderstyle none
	alpha 0.3
	Scale 0.8
	radius 1
	height 2
	+NOBLOCKMAP
	+NOGRAVITY
	+NOEXTREMEDEATH
	+FORCEXYBILLBOARD
	+DONTSPLASH
	-EXPLODEONWATER
	+INVISIBLE
	Gravity 0.01
	DamageType Bullet
	Decal "BulletDecalNew1"
	states
	{
	Spawn:
		TNT1 A 0
		TNT1 A 0 A_SpawnItemEx ("RicoChet",0,0,-5,0,0,0,0,SXF_NOCHECKPOSITION,0)
	Melee:
		TNT1 A 1
		stop
	}
}

