ACTOR EmptyBrass
{
	Height 2
	Radius 2
	Speed 6
	Scale 0.15
	Mass 0
	Projectile
	BounceType "doom"
	BounceFactor 0.3
	DeathSound "weapons/casing"
	BounceSound "weapons/casing"
	-NOGRAVITY
	-DROPOFF
	-CANBOUNCEWATER
	+WINDTHRUST
	+CLIENTSIDEONLY
	+MOVEWITHSECTOR
	+FORCEXYBILLBOARD
	+NOTDMATCH
	+GHOST
	+FLOORCLIP
	+THRUACTORS
	States
	{
	Spawn:
		TNT1 A 0
		TNT1 A 0 A_JumpIf(waterlevel > 1, "Splash")
		C4S3 FFGGHHAABBCCDDEEFFGGHHAABBCCDDEEFFGGHH 1
		C4S3 FFGGHHAABBCCDDEEFFGGHHAABBCCDDEEFFGGHH 1
		Stop

	Death:
		C4S3 I 0
		TNT1 A 0 ACS_NamedExecuteAlways("BDCheckJanitor")//Check Effects
		TNT1 A 0 ACS_NamedExecuteAlways("BDCheckJanitor2")//Check Effects
		LCPJ A 0 A_Jump(256,"Rest1","Rest2","Rest3","Rest4","Rest5","Rest6","Rest7","Rest8")
		Goto Rest1

	Rest1:
		C4S3 I 210
		TNT1 A 0 A_JumpIfInventory("LowGraphicsMode", 1, "null")
		C4S3 I -1
		Loop

	Rest2:
		C4S3 J 210
		TNT1 A 0 A_JumpIfInventory("LowGraphicsMode", 1, "null")
		C4S3 J -1
		Loop

	Rest3:
		C4S3 K 210
		TNT1 A 0 A_JumpIfInventory("LowGraphicsMode", 1, "null")
		C4S3 K -1
		Loop
	Rest4:
		C4S3 L 210
		TNT1 A 0 A_JumpIfInventory("LowGraphicsMode", 1, "null")
		C4S3 L -1
		Loop

	Rest5:
		C4S3 M 210
		TNT1 A 0 A_JumpIfInventory("LowGraphicsMode", 1, "null")
		C4S3 M -1
		Loop

	Rest6:
		C4S3 I 210
		TNT1 A 0 A_JumpIfInventory("LowGraphicsMode", 1, "null")
		C4S3 I -1
		Loop

	Rest7:
		C4S3 J 210
		TNT1 A 0 A_JumpIfInventory("LowGraphicsMode", 1, "null")
		C4S3 J -1
		Loop

	Rest8:
		C4S3 K 210
		TNT1 A 0 A_JumpIfInventory("LowGraphicsMode", 1, "null")
		C4S3 K -1
		Loop

	Splash:
		TNT1 A 0
		TNT1 A 0 A_SpawnItem("UnderwaterEmptyBrass")
		Stop
	}
}


