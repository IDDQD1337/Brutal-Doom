Actor DopeFish : CommanderKeen
{
	PainSound "DopeFish/Pain"
	DeathSound "DopeFish/Pain"
	States
	{
	Spawn:
		DOPE A -1
		Loop

	Pain:
		DOPE B 4
		DOPE B 8 A_Pain
		Goto Spawn

	Death:
		DOPE B 6 
		DOPE CD 6 A_Scream
		DOPE E 6 A_KeenDie
		Stunned:
		DOPE FGFG 6 
		Loop
	}
}

Actor DemonWitch: BossBrainReplacer
{
	Health 500
	Mass 10000000
	PainChance 255
	PainSound "Witch/Pain"
	DeathSound "Witch/death"
	States
	{
	Spawn:
		WTCH A 100 
		Loop

	Pain:
		WTCH B 36 A_BrainPain
		Goto Spawn

	Death:
		WTCH B 100 Bright A_BrainScream
		WTCH BB 10 Bright 
		WTCH B -1 Bright A_BrainDie
		stop
	}
}

