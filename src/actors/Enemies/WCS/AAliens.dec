	Obituary "$OB_ANCIENTALIEN"
	Tag "$TAGBD_ACIENTALIEN" 
	+USEKILLSCRIPTS
	States
	{
	Spawn:
		AAAG AABBCCDDEEFF 2 A_Look
		Loop

	See:
		AAAG AABBCCDDEEFF 2 A_CHASE
		Loop

	Missile:
		TNT1 A 0 A_Jump(96,"Avoid")
		TNT1 AAA 0
		A1AG A 3 A_FaceTarget
		A1AG B 7 A_FaceTarget
