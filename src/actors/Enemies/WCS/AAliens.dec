{
	Game Doom
	Monster 
	Radius 20
	Height 50
	Health 100
	Mass 1500
	Speed 6
	+FLOAT +NOGRAVITY +FORCEXYBILLBOARD +DONTFALL +NOBLOOD +DONTHARMSPECIES
	SeeSound "AlienGuard/See"
	PainSound "alieno/pain"
